import java.util.* ;
import java.io.*; 
public class Solution 
{
    public static void swap(int[] nums,int i,int j){
        int temp = nums[i];
        nums[i] = nums[j];
        nums[j] = temp;
    }

    public static void sort012(int[] arr)
    {
        int low = 0;
        int mid = 0;
        int high = arr.length-1;
        while(mid<=high){
            if(arr[mid]==0){
                swap(arr,mid,low);
                low++;
                mid++;
            }
            else if(arr[mid]==2){
                swap(arr,high,mid);
                high--;
            }
            else{
                mid++;
            }
        }
    }
}
